{
  "name": "cknow/laravel-money",
  "description": "Laravel Money",
  "type": "library",
  "keywords": [
    "laravel",
    "money",
    "currency"
  ],
  "homepage": "https://github.com/cknow/laravel-money",
  "license": "MIT",
  "authors": [
    {
      "name": "Ricardo Gobbo de Souza",
      "email": "ricardogobbosouza@yahoo.com.br",
      "role": "Developer"
    }
  ],
  "support": {
    "email": "ricardo@clicknow.com.br",
    "issues": "https://github.com/cknow/laravel-money/issues",
    "source": "https://github.com/cknow/laravel-money"
  },
  "require": {
    "php": ">=7.0",
    "ext-intl": "*",
    "illuminate/support": "~5.5",
    "illuminate/view": "~5.5",
    "moneyphp/money": "^3.0"
  },
  "require-dev": {
    "mockery/mockery": "~0.9",
    "phpunit/phpunit": "~6.0",
    "graham-campbell/testbench": "~4.0",
    "phpro/grumphp": "~0.12",
    "sebastian/phpcpd": "~3.0",
    "squizlabs/php_codesniffer": "~3.0",
    "jakub-onderka/php-parallel-lint": "~0.9",
    "phpmd/phpmd": "~2.6",
    "povils/phpmnd": "~1.1",
    "nikic/php-parser": "~3.1",
    "phpstan/phpstan": "~0.8",
    "sensiolabs/security-checker": "~4.1",
    "friendsofphp/php-cs-fixer": "~2.5"
  },
  "autoload": {
    "files": [
      "src/helpers.php"
    ],
    "psr-4": {
      "Cknow\\Money\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Cknow\\Money\\": "tests/"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "scripts": {
    "lint": [
      "php-cs-fixer fix"
    ],
    "test": [
      "grumphp run"
    ]
  },
  "extra": {
    "laravel": {
        "providers": [
            "Cknow\\Money\\MoneyServiceProvider"
        ]
    }
  }
}
